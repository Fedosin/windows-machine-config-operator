{
	"CniVersion":"0.2.0",
	"Name":"OVNKubernetesHybridOverlayNetwork",
	"Type":"win-overlay",
	"Capabilities":{
		"Dns":true
	},
	"Ipam":{
		"Type":"host-local",
		"Subnet":"ovn_host_subnet"
	},
	"Policies":[
		{
			"Name":"EndpointPolicy",
			"Value":{
				"Type":"OutBoundNAT",
				"ExceptionList":[
					"service_network_cidr"
				],
				"DestinationPrefix":"",
				"NeedEncap":false
			}
		},
		{
			"Name":"EndpointPolicy",
			"Value":{
				"Type":"ROUTE",
				"ExceptionList":[],
				"DestinationPrefix":"service_network_cidr",
				"NeedEncap":true
			}
		}
	]
}
